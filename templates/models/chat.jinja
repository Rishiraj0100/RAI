function checkTextarea() {
    const textareaValue = $('#input{{ model.name }}').val().trim();
    if (textareaValue === '') {
        $("#speech_container").removeClass("hidden");
        $("#send_container").addClass("hidden");
    } else {
        $("#speech_container").addClass("hidden");
        $("#send_container").removeClass("hidden");
    }
}

$(document).on("keydown", (e) => {
    setTimeout(()=>{checkTextarea();}, 200);
});
$("#input{{ model.name }}").on("keydown", (e) => {
    if (e.key === "Enter" && e.shiftKey) {
    return; // let the browser insert newline
  }

  // Detect Enter alone â†’ "send" behavior
  if (e.key === "Enter") {
    e.preventDefault(); // stop newline
    sendToServer();      // your custom function
  }
});

let recognizedText = '';

// Speech recognition setup
const recognition = new (window.SpeechRecognition || window.webkitSpeechRecognition)();
recognition.continuous = false;
recognition.interimResults = true;

recognition.onresult = function(event) {
    recognizedText = event.results[0][0].transcript;
    document.getElementById('input{{ model.name }}').value = recognizedText;
    checkTextarea();
}

recognition.onerror = function(event) {
    alert('Error occurred: ' + event.error);
}
var processing = false;
function startRecording() {
    recognition.start();
}
marked.setOptions({
    highlight: (code) => code
});
function delay(ms) {
    return new Promise(resolve => setTimeout(resolve, ms));
}
const msgs = [];

function sendToServer() {
    if (processing){return;}
    processing = true;
    const textareaValue = $('#input{{ model.name }}').val().trim();

    $("#speech_container").removeClass("hidden");
    $("#send_container").addClass("hidden");
    $("#null_section").addClass("hidden");
    $("#chat_section").removeClass("hidden");
    $("#chats").html((i, t) => {return t+ usr_temp.replace("[[msg]]", marked.parse(textareaValue));});
    document.getElementById('input{{ model.name }}').value = "";
    document.querySelectorAll('pre code').forEach(block => {
        hljs.highlightElement(block);
    });
    $('#input{{ model.name }}').attr("disabled", true).attr("placeholder", "Processing  ____");
    let iid = setInterval(async () => {
        $('#input{{ model.name }}').attr("placeholder", processing?"Processing  -___" : "Ask Anything");
        await delay(200);
        $('#input{{ model.name }}').attr("placeholder", processing?"Processing  _-__" : "Ask Anything");
        await delay(200);
        $('#input{{ model.name }}').attr("placeholder", processing?"Processing  __-_" : "Ask Anything");
        await delay(200);
        $('#input{{ model.name }}').attr("placeholder", processing?"Processing  ___-" : "Ask Anything");
        await delay(200);
    }, 800)
    msgs.push({
        role: "user",
        content: textareaValue
    });
    fetch(location.pathname, {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify({ messages: msgs }),
    })
    .then(response => response.json())
    .then(data => {
        $("#chats").html((i, t) => {return t+ ai_temp.replace("[[msg]]", marked.parse(data.response));});
        setTimeout(()=>{speak(data.response);},200);
        msgs.push({
            role: "assistant",
            content: data.response
        });
        document.querySelectorAll('pre code').forEach(block => {
            hljs.highlightElement(block);
        });
        clearInterval(iid);
        processing=false;
        setTimeout(()=>{$('#input{{ model.name }}').attr("disabled", false).attr("placeholder", "Ask Anything");},900);
        $("#cs").scrollTop($("#cs")[0].scrollHeight);
    })
    .catch(error => {
        console.error('Error:', error);
    });
}
function speak(textToSpeak) {
    speechSynthesis.cancel();
    const utterance = new SpeechSynthesisUtterance(textToSpeak);

    // Optional: Customize speech properties
    utterance.voice = speechSynthesis.getVoices().find(voice => voice.name === "Google UK English Female");
    // utterance.rate = 1; // Speed (0.1 to 10)
    utterance.pitch = 1.3; // Pitch (0 to 2)
    // utterance.volume = 1; // Volume (0 to 1)

    window.speechSynthesis.speak(utterance);
}
